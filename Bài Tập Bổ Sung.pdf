        "#9.Kiểm tra số nguyên\n",
        "\n",
        "n = int(input(\"Nhập một số nguyên n: \"))\n",
        "if n < 2:\n",
        "    print(n, \"không phải là số nguyên tố.\")\n",
        "else:\n",
        "    la_nguyen_to = True\n",
        "    for i in range(2, int(n**0.5) + 1):\n",
        "        if n % i == 0:\n",
        "            la_nguyen_to = False\n",
        "            break\n",
        "if la_nguyen_to:\n",
        "  print(n, \"là số nguyên tố.\")\n",
